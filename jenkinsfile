node(){
    stage('Initialization'){
        sh 'echo Dummy Step'
        sh 'sleep 20'
    }
    stage('Testing'){
        parallel LA_Amazon_Linux2: {
            node('LA_Amazon_Linux2'){
                sh 'echo Hello from LA_Amazon_Linux2'
                sh 'sleep 20'
            }            
        },
        master: {
            sh 'echo hello from master'
            sh 'sleep 30'
        }
    }
    stage('Notify People') {
        sh 'echo hello people'
    }
    stage('Notification2'){
	sh 'echo  second notification'
	}
}
